  <wsdl:service name="extractseqService">
    <wsdl:port name="edit.extractseq.derived" binding="impl:edit.extractseq.derivedSoapBinding">
      <wsdlsoap:address location="http://www.dkfz-heidelberg.de/menu/tomcat/as/edit.extractseq.derived"/>
    </wsdl:port>
